{
  "name": "jiling-size-table",
  "version": "1.5.0",
  "type": "module",
  "description": "集领商品部尺码表生成器",
  "author": "师兄",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,css,md}",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "build-electron": "npm run build && electron-builder",
    "create-icon": "node scripts/create-icon.js",
    "pack": "npm run build && npm run create-icon && electron-builder --dir",
    "dist": "npm run build && npm run create-icon && electron-builder",
    "dist:win": "npm run build && npm run create-icon && electron-builder --win",
    "dist:portable": "npm run build && npm run create-icon && electron-builder --win portable",
    "prebuild": "npm run create-icon",
    "clean": "rimraf dist dist-electron node_modules/.vite",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.12.0",
    "better-sqlite3": "^12.2.0",
    "electron-is-dev": "^3.0.1",
    "exceljs": "^4.4.0",
    "framer-motion": "^10.16.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.4.1",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.1.0",
    "babel-jest": "^30.0.4",
    "concurrently": "^8.2.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "png-to-ico": "^2.1.8",
    "prettier": "^3.1.0",
    "terser": "^5.43.1",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.jiling.sizechart",
    "productName": "集领尺码表生成器",
    "copyright": "Copyright © 2025 集领",
    "directories": {
      "output": "dist-electron",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "main.cjs",
      "src/services/backend/**/*",
      "package.json",
      "node_modules/**/*",
      "build/icon.ico",
      "build/app.ico",
      "build/icon.png"
    ],
    "extraFiles": [
      {
        "from": "src/assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraResources": [
      {
        "from": "build/icon.ico",
        "to": "build/icon.ico"
      },
      {
        "from": "build/icon.png", 
        "to": "build/icon.png"
      },
      {
        "from": "build/app.ico",
        "to": "build/app.ico"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico",
      "requestedExecutionLevel": "asInvoker",
      "signAndEditExecutable": false,
      "publisherName": "集领科技"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico", 
      "installerHeaderIcon": "build/icon.ico",
      "createDesktopShortcut": false,
      "createStartMenuShortcut": false,
      "shortcutName": "集领尺码表生成器",
      "menuCategory": false,
      "perMachine": false,
      "artifactName": "集领尺码表生成器-Setup-${version}.exe",
      "deleteAppDataOnUninstall": false,
      "runAfterFinish": true,
      "displayLanguageSelector": false,
      "installerLanguages": ["zh_CN"],
      "language": "2052",
      "include": "build/installer.nsh"
    },
    "portable": {
      "artifactName": "集领尺码表生成器-${version}-portable.exe"
    },
    "electronDownload": {
      "mirror": "https://registry.npmmirror.com/-/binary/electron/"
    },
    "includeSubNodeModules": true,
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "compression": "maximum"
  }
}
